{
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
  },
  "source": {
      "include": ["src"],
      "includePattern": "^((?!bundle|test).)*js$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
  },
  "opts": {
      "destination": "jsdocs",
      "recurse": true,
      "readme": "README.md"
  }
}